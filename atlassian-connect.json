{
  "key": "confluence-theme-test",
  "name": "A test add-on for theme",
  "description": "My very first app",
  "baseUrl": "https://randing89.github.io",
  "authentication": {
    "type": "none"
  },
  "modules": {
    "confluenceThemes": [
      {
        "key": "my-first-theme",
        "routeOverride": true,
        "availableGlobally": true,
        "icon": {
          "width": 110,
          "height": 70,
          "url": "/icon.png"
        },
        "name": {
          "value": "Hello World Theme"
        },
        "description": {
          "value": "My first Connect theme."
        },
        "routes": {
          "spaceview": {
            "url": "/confluence-theme-test/hello.html?key={space.key}"
          }
        }
      }
    ]
  }
}
